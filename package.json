  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.6",
    "@types/node": "20.14.9",
    "@wkovacs64/prettier-config": "4.1.1",
    "eslint": "8.57.0",
    "prettier": "3.3.2",
    "typescript": "5.5.2"
  },
  "peerDependencies": {
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
