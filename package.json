{
  "name": "eslint-plugin-wkovacs64",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 ESLint plugin",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/eslint-plugin-wkovacs64.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-plugin-wkovacs64/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "validate": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "merge": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@wkovacs64/prettier-config": "1.0.2",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "semantic-release": "17.0.4",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "eslint": "^5.16.0 || ^6.1.0"
  }
}
