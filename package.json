    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "eslint": ">= 7.5"
  }
