{
  "name": "eslint-plugin-wkovacs64",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 ESLint plugin",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/eslint-plugin-wkovacs64.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-plugin-wkovacs64/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "validate": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-dom": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "merge": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1",
    "semantic-release": "18.0.1",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "eslint": ">= 7.5"
  }
}
